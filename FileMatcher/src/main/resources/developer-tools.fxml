                    </HBox.margin>
                </Label>
                <ProgressBar fx:id="pbProcess" prefWidth="100.0" progress="0.0" />
            </children>
            <VBox.margin>
                <Insets left="10.0" right="10.0" />
            </VBox.margin>
        </HBox>
    </children>
